{
  "manifest_version": 3,
  "name": "书签导出工具",
  "version": "1.0",
  "description": "将浏览器书签导出为特定数据结构的工具",
  "permissions": [
    "bookmarks",
    "tabs",
    "activeTab"
  ],
  "icons": {
    "128": "icon.svg"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.svg"
  },
  "background": {
    "service_worker": "background.js"
  }
}